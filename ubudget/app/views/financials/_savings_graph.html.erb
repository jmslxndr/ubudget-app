<div id="financials2"></div>
	<script type="text/javascript">
		savingsData = <%= raw @financial.savings_to_datum.to_json if @financial.savings %>;
		nv.addGraph(function() {
	 		var chart = nv.models.pieChart()
	      .x(function(d) { return d.label })
	      .y(function(d) { return d.value })
	      .showLabels(false);

	    d3.select("#financials2")
	    		.append("svg:svg")
	        .datum(savingsData)
	        .transition().duration(350)
	        .call(chart);

	  return chart;
	});
</script>