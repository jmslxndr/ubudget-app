 <%= form_tag 'financials', method: :get do %>
 	<p>
 		<%= label_tag :wage, "Yearly Income" %>
 		<% text_field_tag :wage %>
 	</p>
 	<p>
 		<%= label_tag :interest, "Interest Income" %>
 		<% text_field_tag :interest %>
 	</p>
 	<p>
 		<%= label_tag :investments, "Investment Income" %>
 		<% text_field_tag :investments %>
 	</p>
 	<p>
 		<%= label_tag :misc_income, "Miscellaneous Income" %>
 		<% text_field_tag :misc_income %>
 	</p>

	<%= submit_tag 'Submit Data' %>
<% end %>